<h3 class="text-center"><strong>Search Results</strong></h3>
<br>

<div class="row">
  <div class="col-sm-4">
    <address>
      <strong><%= link @search.biz, to: search_path(@conn, :show, @search.id) %></strong><br>
      <%= if @search.address2 do %>
        <%= @search.address1 || "(street address)" %>, <%= @search.address2 %>
      <% else %>
        <%= @search.address1 || "(street address)"%>
      <% end %>
      <br>
      <%= @search.city || "(city)" %>, <%= @search.state || "(state)" %> <%= @search.zip %>
      <br>
      <br>
      <%= link "New Search", to: search_path(@conn, :index), class: "btn btn-primary btn-xs" %>
    </address>
  </div>
  <div class="col-sm-8" id="">
    <div class="container" id="results">
      <div class="panel panel-info">
        <div class="panel-heading">
          <h3><a href="<%= "http://www.whitepages.com" %>">White Pages</a></h3>
        </div>
        <div class="panel-body">
          <address>
            <strong>
              <div id="res-biz">
                biz
              </div>
            </strong>
            <div id="res-address">
              address
            </div>
            <span id="res-city">city</span>, <span id="res-state">state</span> <span id="res-zip">zip</span>

          </address>
        </div>
        <button id="test-fill" class="btn btn-primary form-control" type="submit">Test</button>
        <div class="panel-footer">
          <a href="<%= "http://www.google.com" %>">Test</a>
        </div>
      </div>
    </div>
  </div>
</div>
